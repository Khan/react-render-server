{
  "name": "react-render-server",
  "version": "0.1.0",
  "description": "A node.js server for server-side rendering Khan Academy react components.",
  "main": "src/main.js",
  "scripts": {
    "serve": "nodemon --use_strict src/main.js",
    "serve_local": "nodemon --use_strict src/main.js -- --host=http://localhost:8080",
    "profile": "node-debug -p 8040 src/main.js --host=http://localhost:8080",
    "test": "mocha --use_strict --reporter spec 'src/*_test.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Khan/react-render-server.git"
  },
  "author": "Jamie Wong <jamie.wong@khanacademy.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Khan/react-render-server/issues"
  },
  "homepage": "https://github.com/Khan/react-render-server#readme",
  "dependencies": {
    "argparse": "^1.0.3",
    "body-parser": "^1.14.2",
    "canvas": "^1.3.6",
    "express": "^4.13.3",
    "jsdom": "^7.2.2",
    "react": "0.14.0",
    "react-dom": "0.14.0",
    "superagent": "^1.6.1"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "node-inspector": "^0.12.5",
    "nodemon": "^1.8.1",
    "superagent-mocker": "^0.3.0",
    "supertest": "^1.1.0"
  }
}
